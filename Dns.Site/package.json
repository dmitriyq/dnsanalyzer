{
  "name": "dns-core",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service build --watch",
    "dtos": "cd src && typescript-ref",
    "postinstall": "npm run build",
    "publish": "vue-cli-service build && dotnet publish -c Release",
    "publish-debug": "vue-cli-service build --mode development && dotnet publish -c Debug"
  },
  "dependencies": {
    "@microsoft/signalr": "^3.1.0-preview1.19508.20",
    "axios": "^0.19.0",
    "date-fns": "^2.5.1",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.5",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.6.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.2",
    "sass": "^1.23.0",
    "sass-loader": "^7.1.0",
    "typescript": "^3.6.3",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
