{
	"name": "dns-core",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"postinstall": "npm run build",
		"dev": "vue-cli-service build --watch",
		"serve": "vue-cli-service serve",
		"dtos": "cd src && typescript-ref",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"publish": "vue-cli-service build && dotnet publish -c Release",
		"publish-debug": "vue-cli-service build --mode development && dotnet publish -c Debug"
	},
	"dependencies": {
		"@microsoft/signalr": "^3.0.0",
		"axios": "^0.19.0",
		"date-fns": "^2.4.1",
		"es6-shim": "^0.35.5",
		"font-awesome": "^4.7.0",
		"promise-polyfill": "8.1.3",
		"vue": "^2.6.10",
		"vue-class-component": "^7.1.0",
		"vue-property-decorator": "^8.2.2",
		"vue-router": "^3.1.3",
		"vuetify": "^2.1.1",
		"whatwg-fetch": "^3.0.0"
	},
	"devDependencies": {
		"@babel/polyfill": "^7.6.0",
		"@vue/cli-plugin-babel": "^3.11.0",
		"@vue/cli-plugin-typescript": "^3.11.0",
		"@vue/cli-service": "^3.11.0",
		"babel-core": "7.0.0-bridge.0",
		"gulp": "^4.0.2",
		"sass": "^1.23.0",
		"sass-loader": "^7.1.0",
		"typescript": "^3.6.3",
		"vue-cli-plugin-vuetify": "^0.6.3",
		"vue-template-compiler": "^2.6.10",
		"webpack-bundle-analyzer": "^3.5.2"
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
