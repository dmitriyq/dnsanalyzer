    "schemas": {
      "NotifyModel": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "notifyType",
          "siteType",
          "userName",
          "value"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "userName": {
            "type": "string"
          },
          "siteType": {
            "type": "string",
            "enum": [
              "None",
              "Auth",
              "Asr",
              "Dns",
              "Tg"
            ]
          },
          "notifyType": {
            "type": "string",
            "enum": [
              "None",
              "Email",
              "Sms",
              "SynoChat",
              "Telegram"
            ]
          },
          "value": {
            "type": "string"
          }
        }
      },
      "ProblemDetails": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "type": {
